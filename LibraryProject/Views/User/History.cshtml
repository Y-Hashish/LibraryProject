@model List<Book>
@{
    ViewData["Title"] = "History";
}

<h1>History</h1>

<table class="table table-bordered table-hover">
    <tr>
        <td>Title</td>
        <td>Auther</td>
        <td>kind</td>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.Author</td>
            
                @foreach(var i in ViewBag.kind)
                {
                    
                    if (item.KindId== i.Id)
                    {
                       <td>@i.Name</td> 
                    }
                }              
        </tr>
    }

</table>

